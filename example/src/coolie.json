{
  "js": {
    "main": [
      "./static/js/app/**/*.js"
    ],
    "coolie-config.js": "./static/js/coolie-config.js",
    "dest": "./static/js/",
    "chunk": [
      "./static/js/libs1/**/*",
      "./static/js/libs2/**/*"
    ]
  },
  "html": {
    "src": [
      "./html/**/*.html"
    ],
    "minify": true
  },
  "css": {
    "dest": "./static/css/",
    "minify": {
      "compatibility": "ie7"
    }
  },
  "resource": {
    "dest": "./static/res/",
    "minify": true
  },
  "copy": [
    "./abc.js"
  ],
  "dest": {
    "dirname": "../dest/",
    "host": "/"
  }
}