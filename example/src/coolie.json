{
  "js": {
    "src": [
      "./static/js/app/**/*.js"
    ],
    "coolie-config.js": "./static/js/coolie-config.js",
    "dest": "./static/js/"
  },
  "html": {
    "src": [
      "./html/**/*.html"
    ],
