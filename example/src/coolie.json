{
  "js": {
    "src": [
      "./static/js/app/**/*.js"
    ],
    "coolie-config.js": "./static/js/coolie-config.js"
  },
  "html": {
    "src": [
      "./html/**/*.html"
    ],
    "minify": true
  },
  "css": {
    "dest": "./static/css/",
    "host": "",
    "minify": {
      "compatibility": "ie7"
    }
  },
  "resource": {
    "src": [
      "./static/img/**/*.*",
      "./static/fonts/**/*.*"
    ],
    "dest": "./static/res/"
  },
  "copy": [],
  "dest": "../dest/"
}